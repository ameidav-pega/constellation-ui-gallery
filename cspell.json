{
  "version": "0.2",
  "import": "@pega/cspell-config",
  "language": "en-US",
  "ignorePaths": [
    "package-lock.json",
    "**/*.tsbuildinfo",
    "**/node_modules/**",
    "lib",
    "**/.DS_Store",
    "**/*.zip",
    "**/*.tgz",
    ".eslintrc.json"
  ],
  "allowCompoundWords": true,
  "words": [
    "camelcase",
    "daygrid",
    "fullcalendar",
    "IBAN",
    "imask",
    "isday",
    "jsbarcode",
    "Leanez",
    "Lenovo",
    "OPENWORKBYHANDLE",
    "pangea",
    "pasteable",
    "pconnect",
    "pcore",
    "PHARMACODE",
    "reactflow",
    "Talia",
    "timegrid"
  ]
}
